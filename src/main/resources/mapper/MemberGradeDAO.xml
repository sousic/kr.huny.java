<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.huny.persistence.MemberGradeDAO">
    <update id="UpdateMemberGradeCount">
        UPDATE membergrade set count = count + 1 where grade = #{grade}
    </update>
    <select id="SelectmemberGrade" resultType="kr.huny.domain.MemberGradeVO">
        SELECT * FROM memberGrade where grade = #{grade}
    </select>
</mapper>