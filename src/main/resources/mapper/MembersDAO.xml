<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.huny.persistence.MembersDAO">
    <select id="login" resultType="kr.huny.domain.MembersVO">
        SELECT * from members where userid = #{userid} and pwdfailcnt = #{pwdfailcntLimit} and isloginblock = 0
    </select>
    <select id="idChecker" resultType="kr.huny.domain.MembersVO">
        SELECT * from members where userid = #{userid}
    </select>
</mapper>